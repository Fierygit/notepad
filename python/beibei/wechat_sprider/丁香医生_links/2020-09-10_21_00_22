            function ajax(obj){
                var url   = obj.url;
                var xhr   = new XMLHttpRequest();

                var data = null;
                if (typeof obj.data == "object"){
                    var d = obj.data;
                    data = [];
                    for(var k in d) {
                        if (d.hasOwnProperty(k)){
                            data.push(k + "=" + encodeURIComponent(d[k]));
                        }
